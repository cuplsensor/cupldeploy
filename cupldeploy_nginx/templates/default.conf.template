server {
    listen 80;

    location / { try_files $uri @app; }
    location @app {
        include uwsgi_params;
        uwsgi_pass localhost:5001;
    }
}

server {
    listen 80;
    server_name cuplsensor.com;
    
    location / {   
        try_files $uri @app; 
    }
    location @app {
        rewrite ^/latest(/.*)$ $1 last;
        include uwsgi_params;
        uwsgi_pass localhost:5001;
    }
}



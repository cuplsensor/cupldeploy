server {
    listen ${NGINX_PORT};
    server_name ${SERVER_NAME};
    
    location / {   
        try_files $uri @app; 
    }
    location @app {
        include uwsgi_params;
        uwsgi_pass cuplbackend:${WSB_PORT};
    }
}



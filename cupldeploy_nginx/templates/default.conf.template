server {
    listen 80;

    location / { try_files $uri @app; }
    location @app {
        include uwsgi_params;
        uwsgi_pass localhost:5001;
    }
}

server {
    server_name cuplsensor.com;
    listen 443 ssl;

    location / { try_files $uri @app; }
    location @app {
        include uwsgi_params;
        uwsgi_pass localhost:5001;
    }
}
